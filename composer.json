{
    "name": "izub/laminas-apple-apns",
    "description": "OOP Laminas wrapper for Apple Push Notification Service",
    "license": "BSD-3-Clause",
    "keywords": [
        "laminas",
        "apns",
        "push",
        "notification",
        "apple"
    ],
    "require": {
        "php": "^7.1",
        "laminas/laminas-json": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.27 || ^6.5.8 || ^7.1.5",
        "phpstan/phpstan": "^1.2",
        "laminas/laminas-coding-standard": "~1.0.0"
    },
    "autoload": {
        "psr-4": {
            "Laminas\\Apple\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaminasTest\\Apple\\": "test/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.4.x-dev",
            "dev-develop": "1.5.x-dev"
        }
    }
}
